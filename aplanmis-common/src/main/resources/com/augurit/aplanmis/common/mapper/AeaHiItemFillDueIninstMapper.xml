<!--0 一些前置信息配置 -->
<mapper namespace="com.augurit.aplanmis.common.mapper.AeaHiItemFillDueIninstMapper" >
<!--1 返回所有字段的SQL字句 -->
<sql id="allColumns" >
    DUE_ININST_ID  dueIninstId,
    FILL_ID  fillId,
    INOUTINST_ID  inoutinstId,
    FILL_OPINION  fillOpinion,
    TIME_LIMIT_TYPE  timeLimitType,
    IS_PASS  isPass,
    ATT_COUNT  attCount,
    PAPER_COUNT  paperCount,
    COPY_COUNT  copyCount,
    IS_NEED_ATT  isNeedAtt,
    CREATER  creater,
    CREATE_TIME  createTime,
    ROOT_ORG_ID  rootOrgId
    </sql>

    <sql id="aliasColumns" >
        ${alias}.DUE_ININST_ID  dueIninstId,
        ${alias}.FILL_ID  fillId,
        ${alias}.INOUTINST_ID  inoutinstId,
        ${alias}.FILL_OPINION  fillOpinion,
        ${alias}.TIME_LIMIT_TYPE  timeLimitType,
        ${alias}.IS_PASS  isPass,
        ${alias}.ATT_COUNT  attCount,
        ${alias}.PAPER_COUNT  paperCount,
        ${alias}.COPY_COUNT  copyCount,
        ${alias}.IS_NEED_ATT  isNeedAtt,
        ${alias}.CREATER  creater,
        ${alias}.CREATE_TIME  createTime,
        ${alias}.ROOT_ORG_ID  rootOrgId
    </sql>

<!--2 根据主键ID查询单个实体 -->
<select id="getAeaHiItemFillDueIninstById" resultType="AeaHiItemFillDueIninst" >
    select
    <include refid="allColumns"/>
    from    AEA_HI_ITEM_FILL_DUE_ININST
    where    DUE_ININST_ID = #{id}
</select>

    <select id="listAeaHiItemFillDueIninstByFillId" resultType="AeaHiItemFillDueIninst" >
        SELECT
        <include refid="aliasColumns">
            <property name="alias" value="due"/>
        </include>,
        tt.MAT_NAME
        FROM aea_hi_item_fill_due_ininst due
        LEFT JOIN (
            SELECT aim.MAT_NAME, ahii.INOUTINST_ID FROM aea_hi_item_inoutinst ahii
            LEFT JOIN aea_item_inout aii ON ahii.ITEM_INOUT_ID = aii.INOUT_ID
            LEFT JOIN aea_item_mat aim ON aim.MAT_ID = aii.MAT_ID
        ) tt ON tt.INOUTINST_ID = due.INOUTINST_ID
        WHERE due.FILL_ID = #{fillId}
    </select>

<!--3 根据条件查询实体list -->
    <select id="listAeaHiItemFillDueIninst" resultType="AeaHiItemFillDueIninst" >
        select
        <include refid="allColumns"/>
        from    AEA_HI_ITEM_FILL_DUE_ININST
    <where>
        <if test="dueIninstId != null AND dueIninstId != ''" >
        AND    DUE_ININST_ID = #{dueIninstId}
        </if>
        <if test="fillId != null AND fillId != ''" >
        AND    FILL_ID = #{fillId}
        </if>
        <if test="inoutinstId != null AND inoutinstId != ''" >
        AND    INOUTINST_ID = #{inoutinstId}
        </if>
        <if test="fillOpinion != null AND fillOpinion != ''" >
        AND    FILL_OPINION = #{fillOpinion}
        </if>
        <if test="timeLimitType != null AND timeLimitType != ''" >
        AND    TIME_LIMIT_TYPE = #{timeLimitType}
        </if>
        <if test="isPass != null AND isPass != ''" >
            AND    IS_PASS = #{isPass}
        </if>
        <if test="attCount != null AND attCount != ''" >
            AND    ATT_COUNT = #{attCount}
        </if>
        <if test="paperCount != null AND paperCount != ''" >
        AND    PAPER_COUNT = #{paperCount}
        </if>
        <if test="copyCount != null AND copyCount != ''" >
        AND    COPY_COUNT = #{copyCount}
        </if>
        <if test="isNeedAtt != null AND isNeedAtt != ''" >
        AND    IS_NEED_ATT = #{isNeedAtt}
        </if>
        <if test="creater != null AND creater != ''" >
        AND    CREATER = #{creater}
        </if>
        <if test="createTime != null AND createTime != ''" >
        AND    CREATE_TIME = #{createTime}
        </if>
        <if test="rootOrgId != null AND rootOrgId != ''" >
        AND    ROOT_ORG_ID = #{rootOrgId}
        </if>
    </where>
    </select>
<!--4 根据条件查询实体list,返回分页对象 -->

<!--5 新增实体对象 -->
<insert id="insertAeaHiItemFillDueIninst" parameterType="AeaHiItemFillDueIninst" >
    insert into AEA_HI_ITEM_FILL_DUE_ININST (
DUE_ININST_ID  ,FILL_ID  ,INOUTINST_ID  ,FILL_OPINION  ,TIME_LIMIT_TYPE  ,IS_PASS  ,ATT_COUNT  ,PAPER_COUNT  ,COPY_COUNT  ,IS_NEED_ATT  ,CREATER  ,CREATE_TIME  ,ROOT_ORG_ID
    )  values  (
#{dueIninstId},#{fillId},#{inoutinstId},#{fillOpinion},#{timeLimitType},#{isPass},#{attCount},#{paperCount},#{copyCount},#{isNeedAtt},#{creater},#{createTime},  #{rootOrgId}
    )
</insert>

    <insert id="batchInsertAeaHiItemFillDueIninst" parameterType="AeaHiItemFillDueIninst" >
        insert into AEA_HI_ITEM_FILL_DUE_ININST (
        DUE_ININST_ID  ,FILL_ID  ,INOUTINST_ID  ,FILL_OPINION  ,TIME_LIMIT_TYPE  ,IS_PASS  ,ATT_COUNT  ,PAPER_COUNT  ,COPY_COUNT  ,IS_NEED_ATT  ,CREATER  ,CREATE_TIME  ,ROOT_ORG_ID
        )  values
        <foreach collection="list" item="item" separator=",">
        (
            #{item.dueIninstId},#{item.fillId},#{item.inoutinstId},#{item.fillOpinion},#{item.timeLimitType},#{isPass},#{attCount},#{item.paperCount},
            #{item.copyCount},#{item.isNeedAtt},#{item.creater},#{item.createTime},  #{item.rootOrgId}
        )
        </foreach>
    </insert>

<!--6 修改实体对象 -->
<update id="updateAeaHiItemFillDueIninst" parameterType="AeaHiItemFillDueIninst" >
    update AEA_HI_ITEM_FILL_DUE_ININST
    <set >
        <if test="dueIninstId != null AND dueIninstId != '' " >
        DUE_ININST_ID = #{dueIninstId},
        </if>
        <if test="fillId != null AND fillId != '' " >
        FILL_ID = #{fillId},
        </if>
        <if test="inoutinstId != null AND inoutinstId != '' " >
        INOUTINST_ID = #{inoutinstId},
        </if>
        <if test="fillOpinion != null AND fillOpinion != '' " >
        FILL_OPINION = #{fillOpinion},
        </if>
        <if test="timeLimitType != null AND timeLimitType != '' " >
        TIME_LIMIT_TYPE = #{timeLimitType},
        </if>
        <if test="isPass != null AND isPass != ''" >
         IS_PASS = #{isPass},
        </if>
        <if test="attCount != null AND attCount != ''" >
         ATT_COUNT = #{attCount},
        </if>
        <if test="paperCount != null AND paperCount != '' " >
        PAPER_COUNT = #{paperCount},
        </if>
        <if test="copyCount != null AND copyCount != '' " >
        COPY_COUNT = #{copyCount},
        </if>
        <if test="isNeedAtt != null AND isNeedAtt != '' " >
        IS_NEED_ATT = #{isNeedAtt},
        </if>
        <if test="creater != null AND creater != '' " >
        CREATER = #{creater},
        </if>
        <if test="createTime != null AND createTime != '' " >
        CREATE_TIME = #{createTime},
        </if>
        <if test="rootOrgId != null AND rootOrgId != '' " >
        ROOT_ORG_ID = #{rootOrgId},
        </if>
    </set>
    where DUE_ININST_ID = #{dueIninstId}
</update>
<!--7 删除实体对象,根据主键ID -->
    <delete id="deleteAeaHiItemFillDueIninst">
        delete from   AEA_HI_ITEM_FILL_DUE_ININST
        where
    DUE_ININST_ID = #{id}
    </delete>
<!--8 删除实体对象列表,根据主键ID列表 -->
