<!--0 一些前置信息配置 -->
<mapper namespace="com.augurit.aplanmis.common.mapper.AeaProjApplyAgentMapper" >
<!--1 返回所有字段的SQL字句 -->
<sql id="allColumns" >
    APPLY_AGENT_ID  applyAgentId,
    AGENT_CODE  agentCode,
    PROJ_INFO_ID  projInfoId,
    UNIT_INFO_ID  unitInfoId,
    APPLY_USER_ID  applyUserId,
    AGENT_STAGE_STATE  agentStageState,
    AGENT_APPLY_STATE  agentApplyState,
    WINDOW_ID  windowId,
    START_TIME  startTime,
    END_TIME  endTime,
    CREATER  creater,
    CREATE_TIME  createTime,
    MODIFIER  modifier,
    MODIFY_TIME  modifyTime,
    ROOT_ORG_ID  rootOrgId
    </sql>

<!--2 根据主键ID查询单个实体 -->
<select id="getAeaProjApplyAgentById" resultType="AeaProjApplyAgent" >
    select
    <include refid="allColumns"/>
    from    AEA_PROJ_APPLY_AGENT
    where    APPLY_AGENT_ID = #{id}
</select>

<!--3 根据条件查询实体list -->
    <select id="listAeaProjApplyAgent" resultType="AeaProjApplyAgent" >
        select
        <include refid="allColumns"/>
        from    AEA_PROJ_APPLY_AGENT
    <where>
        <if test="applyAgentId != null" >
        AND    APPLY_AGENT_ID = #{applyAgentId}
        </if>
        <if test="agentCode != null" >
        AND    AGENT_CODE = #{agentCode}
        </if>
        <if test="projInfoId != null" >
        AND    PROJ_INFO_ID = #{projInfoId}
        </if>
        <if test="unitInfoId != null" >
        AND    UNIT_INFO_ID = #{unitInfoId}
        </if>
        <if test="applyUserId != null" >
        AND    APPLY_USER_ID = #{applyUserId}
        </if>
        <if test="agentStageState != null" >
        AND    AGENT_STAGE_STATE = #{agentStageState}
        </if>
        <if test="agentApplyState != null" >
        AND    AGENT_APPLY_STATE = #{agentApplyState}
        </if>
        <if test="windowId != null" >
        AND    WINDOW_ID = #{windowId}
        </if>
        <if test="startTime != null" >
        AND    START_TIME = #{startTime}
        </if>
        <if test="endTime != null" >
        AND    END_TIME = #{endTime}
        </if>
        <if test="creater != null" >
        AND    CREATER = #{creater}
        </if>
        <if test="createTime != null" >
        AND    CREATE_TIME = #{createTime}
        </if>
        <if test="modifier != null" >
        AND    MODIFIER = #{modifier}
        </if>
        <if test="modifyTime != null" >
        AND    MODIFY_TIME = #{modifyTime}
        </if>
        <if test="rootOrgId != null" >
        AND    ROOT_ORG_ID = #{rootOrgId}
        </if>
    </where>
    </select>
<!--4 根据条件查询实体list,返回分页对象 -->

<!--5 新增实体对象 -->
<insert id="insertAeaProjApplyAgent" parameterType="AeaProjApplyAgent" >
    insert into AEA_PROJ_APPLY_AGENT (
APPLY_AGENT_ID  ,AGENT_CODE  ,PROJ_INFO_ID  ,UNIT_INFO_ID  ,APPLY_USER_ID  ,AGENT_STAGE_STATE  ,AGENT_APPLY_STATE  ,WINDOW_ID  ,START_TIME  ,END_TIME  ,CREATER  ,CREATE_TIME  ,MODIFIER  ,MODIFY_TIME  ,ROOT_ORG_ID  
    )  values  (
#{applyAgentId},#{agentCode},#{projInfoId},#{unitInfoId},#{applyUserId},#{agentStageState},#{agentApplyState},#{windowId},#{startTime},#{endTime},#{creater},#{createTime},#{modifier},#{modifyTime},  #{rootOrgId}
    )
</insert>
<!--6 修改实体对象 -->
<update id="updateAeaProjApplyAgent" parameterType="AeaProjApplyAgent" >
    update AEA_PROJ_APPLY_AGENT
    <set >
        <if test="applyAgentId != null" >
        APPLY_AGENT_ID = #{applyAgentId},
        </if>
        <if test="agentCode != null" >
        AGENT_CODE = #{agentCode},
        </if>
        <if test="projInfoId != null" >
        PROJ_INFO_ID = #{projInfoId},
        </if>
        <if test="unitInfoId != null" >
        UNIT_INFO_ID = #{unitInfoId},
        </if>
        <if test="applyUserId != null" >
        APPLY_USER_ID = #{applyUserId},
        </if>
        <if test="agentStageState != null" >
        AGENT_STAGE_STATE = #{agentStageState},
        </if>
        <if test="agentApplyState != null" >
        AGENT_APPLY_STATE = #{agentApplyState},
        </if>
        <if test="windowId != null" >
        WINDOW_ID = #{windowId},
        </if>
        <if test="startTime != null" >
        START_TIME = #{startTime},
        </if>
        <if test="endTime != null" >
        END_TIME = #{endTime},
        </if>
        <if test="creater != null" >
        CREATER = #{creater},
        </if>
        <if test="createTime != null" >
        CREATE_TIME = #{createTime},
        </if>
        <if test="modifier != null" >
        MODIFIER = #{modifier},
        </if>
        <if test="modifyTime != null" >
        MODIFY_TIME = #{modifyTime},
        </if>
        <if test="rootOrgId != null" >
        ROOT_ORG_ID = #{rootOrgId},
        </if>
    </set>
    where APPLY_AGENT_ID = #{applyAgentId}
</update>
<!--7 删除实体对象,根据主键ID -->
    <delete id="deleteAeaProjApplyAgent">
        delete from   AEA_PROJ_APPLY_AGENT
        where
    APPLY_AGENT_ID = #{id}
    </delete>
<!--8 删除实体对象列表,根据主键ID列表 -->
<!--9 返回所有字段的对象map,用于级联 -->
    <resultMap type="AeaProjApplyAgent" id="AeaProjApplyAgentMap">
        <id property="applyAgentId" column="APPLY_AGENT_ID"/>
        <result property="agentCode" column="AGENT_CODE"/>
        <result property="projInfoId" column="PROJ_INFO_ID"/>
        <result property="unitInfoId" column="UNIT_INFO_ID"/>
        <result property="applyUserId" column="APPLY_USER_ID"/>
        <result property="agentStageState" column="AGENT_STAGE_STATE"/>
        <result property="agentApplyState" column="AGENT_APPLY_STATE"/>
        <result property="windowId" column="WINDOW_ID"/>
        <result property="startTime" column="START_TIME"/>
        <result property="endTime" column="END_TIME"/>
        <result property="creater" column="CREATER"/>
        <result property="createTime" column="CREATE_TIME"/>
        <result property="modifier" column="MODIFIER"/>
        <result property="modifyTime" column="MODIFY_TIME"/>
        <result property="rootOrgId" column="ROOT_ORG_ID"/>
    </resultMap>
</mapper>