<!--0 一些前置信息配置 -->
<mapper namespace="com.augurit.aplanmis.common.mapper.AeaProjApplySplitMapper" >
<!--1 返回所有字段的SQL字句 -->
<sql id="allColumns" >
    APPLY_SPLIT_ID  applySplitId,
    PROJ_INFO_ID  projInfoId,
    UNIT_INFO_ID  unitInfoId,
    APPLY_USER_ID  applyUserId,
    APPLY_ACCOUNT_UID  applyAccountUid,
    STAGE_NO  stageNo,
    STAGE_ID  stageId,
    FRONT_STAGE_PROJ_INFO_ID  frontStageProjInfoId,
    LEADER_ORG_ID  leaderOrgId,
    LEADER_USER_ID  leaderUserId,
    APPLY_STATE  applyState,
    START_TIME  startTime,
    END_TIME  endTime,
    SPLIT_MEMO  splitMemo,
    IS_DELETED  isDeleted,
    CREATER  creater,
    CREATE_TIME  createTime,
    MODIFIER  modifier,
    MODIFY_TIME  modifyTime,
    ROOT_ORG_ID  rootOrgId
    </sql>

<!--2 根据主键ID查询单个实体 -->
<select id="getAeaProjApplySplitById" resultType="AeaProjApplySplit" >
    select
    <include refid="allColumns"/>
    from    AEA_PROJ_APPLY_SPLIT
    where    APPLY_SPLIT_ID = #{id}
    AND  IS_DELETED = '0'
</select>

<!--3 根据条件查询实体list -->
    <select id="listAeaProjApplySplit" resultType="AeaProjApplySplit" >
        select
        <include refid="allColumns"/>
        from    AEA_PROJ_APPLY_SPLIT
    <where>
        <if test="applySplitId != null" >
        AND    APPLY_SPLIT_ID = #{applySplitId}
        </if>
        <if test="projInfoId != null" >
        AND    PROJ_INFO_ID = #{projInfoId}
        </if>
        <if test="unitInfoId != null" >
        AND    UNIT_INFO_ID = #{unitInfoId}
        </if>
        <if test="applyUserId != null" >
        AND    APPLY_USER_ID = #{applyUserId}
        </if>
        <if test="applyAccountUid != null" >
        AND    APPLY_ACCOUNT_UID = #{applyAccountUid}
        </if>
        <if test="stageNo != null" >
        AND    STAGE_NO = #{stageNo}
        </if>
        <if test="stageId != null" >
        AND    STAGE_ID = #{stageId}
        </if>
        <if test="frontStageProjInfoId != null" >
        AND    FRONT_STAGE_PROJ_INFO_ID = #{frontStageProjInfoId}
        </if>
        <if test="leaderOrgId != null" >
        AND    LEADER_ORG_ID = #{leaderOrgId}
        </if>
        <if test="leaderUserId != null" >
        AND    LEADER_USER_ID = #{leaderUserId}
        </if>
        <if test="applyState != null" >
        AND    APPLY_STATE = #{applyState}
        </if>
        <if test="startTime != null" >
        AND    START_TIME = #{startTime}
        </if>
        <if test="endTime != null" >
        AND    END_TIME = #{endTime}
        </if>
        <if test="splitMemo != null" >
        AND    SPLIT_MEMO = #{splitMemo}
        </if>
        <if test="isDeleted != null" >
        AND    IS_DELETED = #{isDeleted}
        </if>
        <if test="creater != null" >
        AND    CREATER = #{creater}
        </if>
        <if test="createTime != null" >
        AND    CREATE_TIME = #{createTime}
        </if>
        <if test="modifier != null" >
        AND    MODIFIER = #{modifier}
        </if>
        <if test="modifyTime != null" >
        AND    MODIFY_TIME = #{modifyTime}
        </if>
        <if test="rootOrgId != null" >
        AND    ROOT_ORG_ID = #{rootOrgId}
        </if>
      AND  IS_DELETED = '0'
    </where>
    </select>
<!--4 根据条件查询实体list,返回分页对象 -->

<!--5 新增实体对象 -->
<insert id="insertAeaProjApplySplit" parameterType="AeaProjApplySplit" >
    insert into AEA_PROJ_APPLY_SPLIT (
APPLY_SPLIT_ID  ,PROJ_INFO_ID  ,UNIT_INFO_ID  ,APPLY_USER_ID  ,APPLY_ACCOUNT_UID  ,STAGE_NO  ,STAGE_ID  ,FRONT_STAGE_PROJ_INFO_ID  ,LEADER_ORG_ID  ,LEADER_USER_ID  ,APPLY_STATE  ,START_TIME  ,END_TIME  ,SPLIT_MEMO  ,IS_DELETED  ,CREATER  ,CREATE_TIME  ,MODIFIER  ,MODIFY_TIME  ,ROOT_ORG_ID  
    )  values  (
#{applySplitId},#{projInfoId},#{unitInfoId},#{applyUserId},#{applyAccountUid},#{stageNo},#{stageId},#{frontStageProjInfoId},#{leaderOrgId},#{leaderUserId},#{applyState},#{startTime},#{endTime},#{splitMemo},#{isDeleted},#{creater},#{createTime},#{modifier},#{modifyTime},  #{rootOrgId}
    )
</insert>
<!--6 修改实体对象 -->
<update id="updateAeaProjApplySplit" parameterType="AeaProjApplySplit" >
    update AEA_PROJ_APPLY_SPLIT
    <set >
        <if test="applySplitId != null" >
        APPLY_SPLIT_ID = #{applySplitId},
        </if>
        <if test="projInfoId != null" >
        PROJ_INFO_ID = #{projInfoId},
        </if>
        <if test="unitInfoId != null" >
        UNIT_INFO_ID = #{unitInfoId},
        </if>
        <if test="applyUserId != null" >
        APPLY_USER_ID = #{applyUserId},
        </if>
        <if test="applyAccountUid != null" >
        APPLY_ACCOUNT_UID = #{applyAccountUid},
        </if>
        <if test="stageNo != null" >
        STAGE_NO = #{stageNo},
        </if>
        <if test="stageId != null" >
        STAGE_ID = #{stageId},
        </if>
        <if test="frontStageProjInfoId != null" >
        FRONT_STAGE_PROJ_INFO_ID = #{frontStageProjInfoId},
        </if>
        <if test="leaderOrgId != null" >
        LEADER_ORG_ID = #{leaderOrgId},
        </if>
        <if test="leaderUserId != null" >
